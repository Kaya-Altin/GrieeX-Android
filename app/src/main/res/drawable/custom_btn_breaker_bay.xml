<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <stroke android:width="1.0dip" android:color="#ff3b5357" />
            <gradient android:startColor="#ff151f22" android:endColor="#ff576f75" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <stroke android:width="1.0dip" android:color="#ff3b5357" />
            <solid android:color="#ff31535c" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <stroke android:width="1.0dip" android:color="#ff3b5357" />
            <gradient android:startColor="#ff6d8b93" android:endColor="#ff31535c" android:angle="-90.0" />
        </shape>
    </item>
</selector>
