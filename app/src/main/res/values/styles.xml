    <style name="Divider">
        <item name="android:background">?android:listDivider</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginTop">5dip</item>
        <item name="android:layout_marginBottom">5dip</item>
    </style>

    <style name="Card"/>

    <style name="Card.Title">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">?android:attr/colorPrimaryDark</item>
    </style>

    <style name="foodRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingstars</item>
    </style>
    <style name="btnStyleBlackpearl" parent="@android:style/Widget.Button">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:background">@drawable/custom_btn_black_pearl</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.6</item>
    </style>

    <style name="btnStyleBreakerBay" parent="@android:style/Widget.Button">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/custom_btn_breaker_bay</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.6</item>
    </style>

    <style name="SideIndexOverlayText">
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:textColor">#801ca0f2</item>
        <item name="android:textSize">12sp</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="btnStyleRadioButton" parent="@android:style/Widget.Button">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">?attr/colorDrawerText</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.6</item>
        <item name="android:background">@drawable/selector_toggle_button</item>
        <item name="android:padding">3dip</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/scale_up</item>
        <item name="android:windowExitAnimation">@anim/scale_down</item>
    </style>

    <style name="ActionBarThemeOverlay" parent="">
        <item name="android:textColorPrimary">@color/white</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlHighlight">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
    </style>

